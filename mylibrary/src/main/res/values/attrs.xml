<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SwipeBackLayout">
        <attr name="edge_size" format="dimension"/>
        <attr name="edge_flag">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="bottom" value="2" />
            <enum name="all" value="3" />
        </attr>
        <attr name="shadow_left" format="reference"/>
        <attr name="shadow_right" format="reference"/>
        <attr name="shadow_bottom" format="reference"/>
    </declare-styleable>

    <attr name="SwipeBackLayoutStyle" format="reference"/>

    <!-- actionbar 设置custom view里的后退按钮 -->
    <attr name="ActionBarCustomViewNavigationup" format="reference"></attr>

    <declare-styleable name="SolidButton">
        <attr name="solid_press_true_drawable" format="reference"/>
        <attr name="solid_press_false_drawable" format="reference"/>
        <attr name="solid_press_true_color" format="color"/>
        <attr name="solid_press_false_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="StrokeButton">
        <attr name="stroke_press_true_drawable" format="reference"/>
        <attr name="stroke_press_false_drawable" format="reference"/>
        <attr name="stroke_press_true_color" format="color"/>
        <attr name="stroke_press_false_color" format="color"/>
    </declare-styleable>
</resources>